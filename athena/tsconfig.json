{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "target": "ESNext",
        "noEmit": true,
        "composite": false,
        "declaration": false,
        "declarationMap": false,
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "moduleResolution": "node",
        "jsx": "preserve"
    },
    "include": [
        "src"
    ]
}